\mode<presentation>

\definecolor{navyblue}{RGB}{0, 47, 92}
\definecolor{green}{HTML}{8E6C15}
\definecolor{red}{HTML}{780309}
\definecolor{lightgray}{HTML}{F2F2F2}

\setbeamercolor{structure}{fg=gray, bg=white}
\setbeamercolor{frametitle}{fg=navyblue}
\setbeamercolor{framesubtitle}{fg=navyblue}
\setbeamercolor{alerted text}{fg=red}

% block color settings
\setbeamercolor{block title}{fg=white,bg=navyblue}
\setbeamercolor{block body}{parent=normal text,bg=lightgray, fg=black}

\setbeamercolor{block title example}{fg=white,bg=green}
\setbeamercolor{block body example}{parent=normal text,bg=lightgray, fg=black}

\setbeamercolor{block title alerted}{fg=white, bg=red}
\setbeamercolor{block body alerted}{parent=normal text,bg=lightgray, fg=black}

\setbeamercolor{title}{fg=navyblue}
\setbeamercolor{subtitle}{fg=navyblue}
\setbeamercolor{footline}{fg=white, bg=navyblue}

\setbeamercolor{itemize item}{fg=navyblue}
\setbeamercolor{itemize subitem}{fg=navyblue}
\setbeamercolor{itemize subsubitem}{fg=navyblue}

\setbeamertemplate{itemize item}[triangle]
\setbeamertemplate{itemize subitem}[circle]
\setbeamertemplate{itemize subsubitem}[square]


\setbeamercolor{enumerate item}{fg=navyblue}
\setbeamertemplate{enumerate item}[]

\mode
<all>